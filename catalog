<system systemId="http://docbook.sourceforge.net/release/xsl/current/common/targetdatabase.dtd" 
      uri="docbook/dtds/targetdatabase.dtd"/>

  
<!-- the following resolves a 'fake' url prefix locally - used in entities file -->  
  
<rewriteURI
     uriStartString="http://root/"
     rewritePrefix="./" />

